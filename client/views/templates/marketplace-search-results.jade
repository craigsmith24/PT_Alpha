.row
    .columns.large-11.large-centered

        .row.row-listing-container
            .columns.large-6
                .item(itemtype='http://schema.org/Enumeration', style='position:relative; margin-bottom: 12px')
                    .listing(data-long='',data-lat='', data-name='')
                        ul.inline-list(style='margin:0')
                            li.left(style='margin:0')
                                h6(style='margin:0')
                                    small THE CLIFT HOTEL
                                h5(style='margin:0') Sophia Room
                            li.right(style='margin:0')
                                img(src='assets/images/heart.png', width='20', height='20', style='margin-top:12px')
                        // orbit carousel
                        .panel-image.clearfix(style='margin-bottom:8px;')
                            ul.example-orbit(data-orbit)
                                li
                                    img(src='assets/images/hires/room/blux1.jpg')
                                li
                                    img(src='assets/images/hires/room/blux2.jpg')
                                li
                                    img(src='assets/images/hires/room/blux3.jpg')
                                li
                                    img(src='assets/images/hires/room/blux4.jpg')           
                        .profile-image-wrap(style='width:40px; height:40px; overflow:hidden; position:absolute; left:50%; margin-left:-20px; border-radius:60px;top:256px; border:1px solid #fff;')
                            img(src='assets/images/faces/craig.jpg')
                        h6.text-center(style='margin:0;line-height:1em') Craig Smith, 
