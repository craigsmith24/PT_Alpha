        .row.collapse
            .columns.large-3
                i.fi-social-twitter.large
            .columns.small-9
                h6.nomargin Link Twitter
                h6.subheader You have not linked Twitter.
                
        .row.collapse
            .columns.large-3
                i.fi-social-google-plus.large
            .columns.small-9
