.row
    .clean-button-tiny.top-cta.right
        &nbsp; delete account &nbsp;
        i.fi-trash
    .clean-button-tiny.top-cta.right 
        &nbsp; save &nbsp;
        i.fi-save
    .clean-button-tiny.top-cta.right cancel
    h4 USER SETTINGS 
        small | Define Settings For Your Account
    hr
    .columns.large-3
        h5 PROFILE
        hr
        label Name
            input(type='text', placeholder='eg Joe Smith')
     
        label Address
            input(type="text", placeholder='')

        label Phone Number
            input(type="text", placeholder='(555) 555-5555')

        label Email
            input(type="text", placeholder='eg joe@name.com')
        
        label Profile Image
        .row
            .columns.small-5
                .placeholder-image(style='height:60px;width:60px;border:1px solid #aaa')
            .columns.small-7
                .clean-button-tiny choose file

        h5 SECURITY
        hr
        label Password
            input(type='password')
        label New Password
            input(type='password')
        label Re-type NewPassword
            input(type='password')
        .clean-button-tiny Reset Password
        
        h5 GRAPHS
        hr
        h6.dietary Allergies
        .ingredient-tokens.clearfix
            .token.left seafood &nbsp;
                i.fi-x
            .token.left shell fish &nbsp;
                i.fi-x
            .token.left shrimp &nbsp;
                i.fi-x
            .token.left peanuts &nbsp;
                i.fi-x
        .clean-button-tiny edit Ingredients
        
        h6.dietary Dietary Preferences
        .room-tokens.clearfix
            .token.left vegan &nbsp;
                i.fi-x
            .token.left vegetarian &nbsp;
                i.fi-x
        .clean-button-tiny edit Dietary Preferences
        


    .columns.large-3
        h5 NETWORKS & CONTACTS
        hr
        .row.collapse
            .columns.large-3
                i.fi-social-facebook.large
            .columns.small-9
                h6.nomargin Link Facebook
                h6.subheader You have not linked Facebook.
        
        .row.collapse
            .columns.large-3
                i.fi-social-linkedin.large
            .columns.small-9
