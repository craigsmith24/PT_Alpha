        .row.collapse
            .columns.large-3
                i.fi-social-google-plus.large
            .columns.small-9
                h6.nomargin Import Outlook Contacts
                h6.subheader You have not imported from Outlook.
        
        
        
    .columns.large-6
        h5 NOTIFICATIONS
        table.notification(width='100%')
            thead
                tr
